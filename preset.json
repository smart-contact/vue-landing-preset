{
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-service": {
      "version": "next"
    },
    "@vue/cli-plugin-babel": {
      "version": "next"
    },
    "@vue/cli-plugin-e2e-cypress": {
      "version": "next"
    },
    "@vue/cli-plugin-eslint": {
      "version": "next",
      "config": "prettier",
      "lintOn": [
        "save",
        "commit"
      ]
    },
    "vue-cli-plugin-bootstrap-vue": {
      "usePolyfills": false,
      "useScss": true,
      "injectAbstracts": false
    },
    "@smart-contact/vue-cli-plugin-landing-page": { 
      "version": "^1.x.x",
      "prompts": true
    }
  },
  "vueVersion": "2",
  "cssPreprocessor": "dart-sass"
}
