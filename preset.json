{
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-router": { 
      "history": false
     },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-e2e-cypress": {},
    "@vue/cli-plugin-eslint": {
      "config": "prettier",
      "lintOn": [
        "save",
        "commit"
      ]
    },
    "@ascendancyy/vue-cli-plugin-stylelint": {
      "config": "standard",
      "lintStyleOn": ["build", "commit"]
    },
    "vue-cli-plugin-bootstrap-vue": {
      "usePolyfills": false,
      "useScss": true
    },
    "vue-cli-plugin-fontawesome": {},
    "@smart-contact/vue-cli-plugin-landing-page": { "prompts": true }
  },
  "configs": {
    "vue": {
      "configureWebpack": {
        "resolve": { 
          "symlinks": false
        }
      },
      "productionSourceMap": false,
      "outputDir": "dist",
      "css": {
        "loaderOptions": {
          "scss": {
            "additionalData": "@import \"~bootstrap/scss/_functions.scss\"; @import \"@/assets/scss/vendors/bootstrap-vue/_custom.scss\"; @import \"~bootstrap/scss/_variables.scss\"; @import \"~bootstrap/scss/_mixins.scss\"; @import \"~bootstrap-vue/src/_variables.scss\"; @import\"@/assetst/scss/vendors/smartify/_custom.scss\"; @import \"@/assets/scss/abstracts/_functions.scss\";@import \"@/assets/scss/abstracts/_variables.scss\";@import \"@/assets/scss/abstracts/_mixins.scss\";"
          }
        }
      }      
    }
  },
  "vueVersion": "2",
  "cssPreprocessor": "dart-sass"
}