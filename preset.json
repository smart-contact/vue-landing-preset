{
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-eslint": {
      "config": "prettier",
      "lintOn": [
        "save",
        "commit"
      ]
    },
    "vue-cli-plugin-stylelint": {
      "config": "stylelint-config-standard"
    },
    "vue-cli-plugin-bootstrap-vue": {
      "usePolyfills": false,
      "useScss": true
    },
    "vue-cli-plugin-fontawesome": {},
    "@smart-contact/vue-cli-plugin-landing-page": {}
  },
  "configs": {
    "vue": {
      "configureWebpack": {
        "resolve": { 
          "symlinks": false
        }
      },
      "productionSourceMap": false,
      "outputDir": "dist",
      "css": {
        "loaderOptions": {
          "scss": {
            "additionalData": "@import \"@/assets/scss/vendors/bootstrap/_custom.scss\"; @import \"@/assets/scss/vendors/bootstrap/_custom-bootstrap-vue.scss\"; @import \"~bootstrap/scss/_functions.scss\";@import \"~bootstrap/scss/_variables.scss\";@import \"~bootstrap/scss/_mixins.scss\";@import \"~bootstrap-vue/src/_variables.scss\";@import \"@/assets/scss/abstracts/_functions.scss\";@import \"@/assets/scss/abstracts/_variables.scss\";@import \"@/assets/scss/abstracts/_mixins.scss\";"
          }
        }
      }      
    }
  },
  "vueVersion": "2",
  "cssPreprocessor": "dart-sass"
}